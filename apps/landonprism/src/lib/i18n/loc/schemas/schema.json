{
    "title": "JSON schema for #GEEK web application localization",
    "type": "object",
    "properties": {
        "landing": {
            "type": "object",
            "description": "Landing page localization",
            "properties": {
                "common": {
                    "type": "object",
                    "properties": {
                        "btnregister": {
                            "type": "string"
                        },
                        "btnregister_alt": {
                            "type": "string"
                        },
                        "btnask": {
                            "type": "string"
                        },
                        "locru": {
                            "type": "string"
                        },
                        "locen": {
                            "type": "string"
                        },
                        "hashtag": {
                            "type": "string"
                        }
                    },
                    "required": ["btnregister", "btnregister_alt", "locru", "locen", "hashtag"]
                },
                "head": {
                    "type": "object",
                    "properties": {
                        "title": {
                            "type": "string"
                        },
                        "nav": {
                            "type": "object",
                            "properties": {
                                "hackaton_link": {
                                    "type": "string"
                                },
                                "program_link": {
                                    "type": "string"
                                },
                                "speech_link": {
                                    "type": "string"
                                },
                                "aidev_link": {
                                    "type": "string"
                                },
                                "mobiledev_link": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "hackaton_link",
                                "program_link",
                                "speech_link",
                                "aidev_link",
                                "mobiledev_link"
                            ]
                        }
                    },
                    "required": ["title", "nav"]
                },
                "greet": {
                    "type": "object",
                    "properties": {
                        "cow": {
                            "type": "object",
                            "properties": {
                                "frontend": {
                                    "type": "string"
                                },
                                "ai": {
                                    "type": "string"
                                },
                                "highloads": {
                                    "type": "string"
                                },
                                "teaching": {
                                    "type": "string"
                                },
                                "gamedev": {
                                    "type": "string"
                                },
                                "mobile": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "frontend",
                                "ai",
                                "highloads",
                                "teaching",
                                "gamedev",
                                "mobile"
                            ]
                        },
                        "description": {
                            "type": "string"
                        }
                    },
                    "required": ["cow", "description"]
                },
                "about": {
                    "type": "object",
                    "properties": {
                        "windows": {
                            "type": "object",
                            "properties": {
                                "members": {
                                    "type": "object",
                                    "$ref": "./about.window.json"
                                },
                                "money": {
                                    "type": "object",
                                    "$ref": "./about.window.json"
                                },
                                "coffee": {
                                    "type": "object",
                                    "$ref": "./about.window.json"
                                },
                                "tasks": {
                                    "type": "object",
                                    "$ref": "./about.window.json"
                                },
                                "company": {
                                    "type": "object",
                                    "$ref": "./about.window.json"
                                },
                                "lections": {
                                    "type": "object",
                                    "$ref": "./about.window.json"
                                },
                                "under_lections": {
                                    "type": "object",
                                    "$ref": "./about.window.json"
                                },
                                "targetclick": {
                                    "type": "object",
                                    "$ref": "./about.window.json"
                                }
                            },
                            "required": [
                                "members",
                                "money",
                                "coffee",
                                "tasks",
                                "company",
                                "lections",
                                "under_lections",
                                "targetclick"
                            ]
                        }
                    },
                    "required": ["windows"]
                },
                "program": {
                    "type": "object",
                    "properties": {
                        "advs": {
                            "type": "object",
                            "properties": {
                                "opening": {
                                    "type": "string"
                                },
                                "coworking": {
                                    "type": "string"
                                },
                                "lections": {
                                    "type": "string"
                                },
                                "freemic": {
                                    "type": "string"
                                },
                                "hackatons": {
                                    "type": "string"
                                }
                            },
                            "required": ["opening", "coworking", "lections", "freemic", "hackatons"]
                        },
                        "programwindow": {
                            "type": "object",
                            "properties": {
                                "title": {
                                    "type": "string"
                                }
                            },
                            "required": ["title"]
                        },
                        "aboutuswindow": {
                            "type": "object",
                            "properties": {
                                "title": {
                                    "type": "string"
                                },
                                "text": {
                                    "type": "string"
                                }
                            },
                            "required": ["title", "text"]
                        }
                    },
                    "required": ["advs", "programwindow", "aboutuswindow"]
                },
                "partners": {
                    "type": "object",
                    "properties": {
                        "windowtitle": {
                            "type": "string"
                        }
                    },
                    "required": ["windowtitle"]
                }
            },
            "required": ["common", "head", "greet", "about", "program", "partners"]
        },
        "registerform": {
            "type": "object",
            "description": "Registration form localization",
            "properties": {
                "title": {
                    "type": "string",
                    "description": "form title"
                },
                "submit": {
                    "type": "string"
                },
                "labels": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "lastname": {
                            "type": "string"
                        },
                        "birth": {
                            "type": "string"
                        },
                        "region": {
                            "type": "string"
                        },
                        "contacttype": {
                            "type": "string"
                        },
                        "contact": {
                            "type": "string"
                        },
                        "hasteam": {
                            "type": "string"
                        },
                        "teamsel": {
                            "type": "string"
                        },
                        "teamcode": {
                            "type": "string"
                        },
                        "wantteam": {
                            "type": "string"
                        },
                        "teamname": {
                            "type": "string"
                        },
                        "teamsizewarning": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "name",
                        "lastname",
                        "birth",
                        "region",
                        "contacttype",
                        "contact",
                        "hasteam",
                        "teamsel",
                        "teamcode",
                        "wantteam",
                        "teamname",
                        "teamsizewarning"
                    ]
                }
            },
            "required": ["title", "labels"]
        }
    },

    "required": ["landing", "registerform", "submit"]
}
